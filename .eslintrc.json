{
  "env": {
      "browser": true,
      "es2021": true,
      "jquery": true
  },
  "extends": [
      "airbnb-base",
      "eslint:recommended"
  ],
  "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
  },
  "rules": {
      "import/extensions": [0],
      "linebreak-style": 0,
      "no-unused-vars": ["error", {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_"
      }]
  },
  "globals": {
      "cordova": "readonly"
  }
}